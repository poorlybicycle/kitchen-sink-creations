<div class="ingredients">
<h1>Ingredients For Your Pantry</h1>

{{#each ingredient}}
    <div>
        <span>{{ingredient_name}}</span>
        <button class="addIngredient btn btn-outline-info" data-ingredientId="{{id}}">Add to Pantry</button>
    </div>
{{/each}}

<script src="/js/addIngredient.js"></script>